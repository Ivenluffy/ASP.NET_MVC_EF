
@model EMS.Model.UserInfo
@{
    ViewBag.Title = "Edit";
}

@Html.ActionLink("返回列表", "Index", null, new { @class = "btn btn-info" })

<table border="1" cellpadding="0" cellspacing="0" width="500">
    <tr>
        <th>ID</th>
        <th>名称</th>
        <th>部门</th>
    </tr>
    <tr>
        <td>@Model.Id</td>
        <td>@Model.Name</td>
        <td>@(Model.Department == null ? "" : Model.Department.Name)</td>
    </tr>
</table>
